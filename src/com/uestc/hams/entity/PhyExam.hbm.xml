		<!-- doctor属性，本类与User的关系是多对一 -->
		<many-to-one name="doctor" class="User" column="doctorId"></many-to-one>
		
		<property name="examDate" length="100"/>
		
		<!-- ras属性，本类与ResidentArchive关系为多对一  级联删除：当健康档案删除的时候相应的健康体检记录也删除-->
		<many-to-one name="residentArchive" class="ResidentArchive" column="residentArchiveId" cascade="delete"></many-to-one>
		
		<!-- recoder属性，本类与User的关系为多对一 -->
		<many-to-one name="recorder" class="User" column="recorderId"></many-to-one>
		
		<!-- hospitalHistorys属性，本类与HospitalHistory的关系为一对多 -->
		<set name="hospitalHistorys" lazy="false">
			<key column="phyExamHosId"></key>
			<one-to-many class="HospitalHistory"/>
		</set>
		
		<!-- familyDisHistorys属性，本类与FamilyDisHistory的关系为一对多 -->
		<set name="familyDisHistorys" lazy="false">
			<key column="phyExamFamId"></key>
			<one-to-many class="FamilyDisHistory"/>
		</set>
		
		<!-- medications属性，本类与Medication的关系为一对多 -->
		<set name="medications" lazy="false">
			<key column="phyExamId"></key>
			<one-to-many class="Medication"/>
		</set>
